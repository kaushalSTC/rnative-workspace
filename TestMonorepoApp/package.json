{
  "name": "testmonorepoapp",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "start:clear": "expo start --clear",
    "test": "jest",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,md}'",
    "format:check": "prettier --check '**/*.{js,jsx,ts,tsx,json,md}'",
    "pod": "npx pod-install",
    "pod:clean": "cd ios && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData && rm -rf build && cd .. && npx pod-install",
    "android:debug": "cd android && ./gradlew assembleDebug",
    "android:release": "cd android && ./gradlew assembleRelease",
    "android:bundle": "cd android && ./gradlew bundleRelease",
    "android:clean": "cd android && ./gradlew clean",
    "android:clean-release": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "ios:debug": "npx react-native build-ios --mode=Debug",
    "ios:release": "npx react-native build-ios --mode=Release",
    "ios:archive": "cd ios && xcodebuild -workspace TestMonorepoApp.xcworkspace -scheme TestMonorepoApp -configuration Release archive -archivePath TestMonorepoApp.xcarchive",
    "apk:debug": "cd android && ./gradlew assembleDebug && open app/build/outputs/apk/debug/",
    "apk:release": "cd android && ./gradlew assembleRelease && open app/build/outputs/apk/release/",
    "aab:release": "cd android && ./gradlew bundleRelease && open app/build/outputs/bundle/release/",
    "clean": "npm run android:clean && npm run ios:clean && npm run metro:clean && npm run modules:clean",
    "ios:clean": "cd ios && xcodebuild clean && rm -rf build && rm -rf ~/Library/Developer/Xcode/DerivedData",
    "metro:clean": "npx react-native start --reset-cache",
    "modules:clean": "rm -rf node_modules && npm install",
    "pods:clean": "cd ios && rm -rf Pods && rm -rf ~/Library/Developer/Xcode/DerivedData && cd .. && npx pod-install",
    "clean:all": "npm run clean && npm run pods:clean",
    "release:android": "npm run android:clean && npm run android:release",
    "release:ios": "npm run ios:clean && npm run ios:release",
    "info": "npx react-native info",
    "doctor": "npx react-native doctor",
    "flipper": "npx react-native flipper",
    "gradle:wrapper": "cd android && ./gradlew wrapper --gradle-version=8.0.2",
    "gradle:refresh": "cd android && ./gradlew --refresh-dependencies",
    "metro:clear": "npx react-native start --reset-cache",
    "watchman:clear": "watchman watch-del-all",
    "build:development": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:production": "eas build --profile production",
    "submit:production": "eas submit --profile production",
    "postinstall": "npx patch-package"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "expo": "~53.0.20",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-inappbrowser-reborn",
          "react-native-chart-kit",
          "react-native-responsive-screen",
          "expo-random",
          "react-native-material-ripple",
          "react-native-swiper",
          "@apollo/client",
          "@expo/metro-runtime",
          "graphql",
          "graphql-request",
          "lodash",
          "react-native-base64",
          "react-native-otp-textinput"
        ]
      }
    }
  }
}
